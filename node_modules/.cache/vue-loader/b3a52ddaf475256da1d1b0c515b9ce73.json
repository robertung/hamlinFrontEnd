ired\n          placeholder=\"Enter email\"\n        />\n      </b-form-group>\n\n    <b-form-group\n        id=\"password\"\n        label-for=\"password\"\n      >\n        <b-form-input\n          id=\"password\"\n          v-model=\"password\"\n          type=\"password\"\n          required\n          placeholder=\"Enter Password\"\n        />\n      </b-form-group>\n      <b-button type=\"button\" variant=\"primary\" @click=\"login()\">Submit</b-button>\n    </b-form>\n    <div class=\"error\" v-if=\"errorMessage\">\n        <p>Email or Password is incorrect</p>\n    </div>\n    </div>\n</template>\n\n<script lang=\"ts\">\nimport { Component, Vue } from 'vue-property-decorator';\n\n@Component\nexport default class Login extends Vue {\n    private email = '';\n\n    private password = '';\n\n    private errorMessage = false;\n\n    private login(): void {\n      this.errorMessage = false;\n      const payload: {} = {\n        email: this.email,\n        password: this.password,\n      };\n\n      this.$store.dispatch('login', payload)\n        .then((response) => {\n          if (response.status === 401) {\n            this.errorMessage = true;\n          }\n          if (response.status === 200) {\n            this.$router.push({ name: 'Profile' });\n          }\n        });\n    }\n}\n</script>\n\n<style scoped lang=\"scss\">\n    .login-view {\n        max-width: 400px;\n        margin: 50px auto;\n        padding: 20px;\n        background: #fa9b03;\n        border-radius: 5px;\n\n        input {\n            font-family: 'Roboto', sans-serif;\n        }\n\n        .error {\n            text-align: center;\n            margin-top: 35px;\n        }\n    }\n</style>"]}]}