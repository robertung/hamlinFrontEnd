Ed,EAAA,OAAO,EAAP,OAFc;AAGd,EAAA,SAAS,EAAT,SAHc;AAId,EAAA,MAAM,EAAN,MAJc;AAKd,EAAA,QAAQ,EAAR,QALc;AAMd,EAAA,UAAU,EAAV,UANc;AAOd,EAAA,mBAAmB,EAAnB;AAPc,CAAf","sourcesContent":["import axios from 'axios';\nimport * as localForage from \"localforage\";\n// import apiConstants from './constants';\n\nconst apiDomain = process.env.API_DOMAIN;\n\nexport function apiPut(url: any, data: any) {\n\treturn new Promise((resolve, reject) => {\n\t\tlocalForage.getItem('userTokens').then((userTokens: any) => {\n\t\t\tlet apiURL = replaceApiVariables(url, userTokens);\n\t\t\tlet dataQS = JSON.stringify(data);\n\t\t\taxios({\n\t\t\t\tmethod: 'PUT',\n\t\t\t\turl: `${apiDomain}/api/${apiURL}`,\n\t\t\t\tdata: dataQS,\n\t\t\t\theaders: {\n\t\t\t\t\t'Authorization': `Bearer ${userTokens.access_token}`,\n\t\t\t\t\t'Content-Type': 'application/json'\n\t\t\t\t}\n\t\t\t})\n\t\t\t.then((response) => {\n\t\t\t\tresolve(response);\n\t\t\t})\n\t\t\t.catch((error) => {\n\t\t\t\treject(error.response);\n\t\t\t});\n\t\t});\n\t});\n}\n\nexport function apiPost(url: any, data: any) {\n\treturn new Promise((resolve, reject) => {\n\t\tlocalForage.getItem('userTokens').then((userTokens: any) => {\n\t\t\tlet apiURL = replaceApiVariables(url, userTokens);\n\t\t\tlet dataQS = JSON.stringify(data);\n\t\t\taxios({\n\t\t\t\tmethod: 'POST',\n\t\t\t\turl: `${apiDomain}/api/${apiURL}`,\n\t\t\t\tdata: dataQS,\n\t\t\t\theaders: {\n\t\t\t\t\t'Authorization': `Bearer ${userTokens.access_token}`,\n\t\t\t\t\t'Content-Type': 'application/json'\n\t\t\t\t}\n\t\t\t})\n\t\t\t\t.then((response) => {\n\t\t\t\t\tresolve(response);\n\t\t\t\t})\n\t\t\t\t.catch((error) => {\n\t\t\t\t\treject(error.response);\n\t\t\t\t});\n\t\t});\n\t});\n}\n\n\n\n\n\n\nexport function apiDelete(url, data) {\n\treturn new Promise((resolve, reject) => {\n\t\tlocalForage.getItem('userTokens').then((userTokens) => {\n\t\t\tlet apiURL = replaceApiVariables(url, userTokens);\n\t\t\tlet dataQS = JSON.stringify(data);\n\t\t\taxios({\n\t\t\t\tmethod: 'DELETE',\n\t\t\t\turl: `${apiDomain}/api/${apiURL}`,\n\t\t\t\tdata: dataQS,\n\t\t\t\theaders: {\n\t\t\t\t\t'Authorization': `Bearer ${userTokens.access_token}`,\n\t\t\t\t\t'Content-Type': 'application/json'\n\t\t\t\t}\n\t\t\t})\n\t\t\t.then((response) => {\n\t\t\t\tresolve(response);\n\t\t\t})\n\t\t\t.catch((error) => {\n\t\t\t\treject(error.response);\n\t\t\t});\n\t\t});\n\t});\n}\n\nexport function apiGet(url: any, params: any) {\n\treturn new Promise((resolve, reject) => {\n\t\tlocalForage.getItem('userTokens').then((userTokens: any) => {\n\t\t\tlet apiURL = replaceApiVariables(url, userTokens);\n\t\t\tlet dataQS = JSON.stringify(params);\n\t\t\taxios({\n\t\t\t\tmethod: 'DELETE',\n\t\t\t\turl: `${apiDomain}/api/${apiURL}`,\n\t\t\t\tdata: dataQS,\n\t\t\t\theaders: {\n\t\t\t\t\t'Authorization': `Bearer ${userTokens.access_token}`,\n\t\t\t\t\t'Content-Type': 'application/json'\n\t\t\t\t}\n\t\t\t})\n\t\t\t.then((response) => {\n\t\t\t\tresolve(response);\n\t\t\t})\n\t\t\t.catch((error) => {\n\t\t\t\treject(error.response);\n\t\t\t});\n\t\t});\n\t});\n\n}\n\n\nexport function apiLogin(payload: any) {\n\tlet email = payload.email;\n\tlet password = payload.password;\n\treturn new Promise((resolve, reject) => {\n\t\taxios({\n\t\t\tmethod: 'post',\n\t\t\turl: `${apiDomain}/login`,\n\t\t\t// url: apiConstants.baseURL + 'oauth/token',\n\t\t\theaders: {\n\t\t\t\t'Content-Type': 'application/json; charset=utf-8'\n\t\t\t},\n\t\t\tdata: {\n\t\t\t\tgrant_type: 'password',\n\t\t\t\tclient_id: 'testclient',\n\t\t\t\tclient_secret: 'testpass',\n\t\t\t\temail: email,\n\t\t\t\tpassword: password\n\t\t\t\t// password: btoa(password)\n\t\t\t}\n\t\t})\n\t\t.then(function (response) {\n\t\t\tresolve(response.data);\n\t\t})\n\t\t.catch(function (error) {\n\t\t\treject(error.response);\n\t\t});\n\n\t});\n}\n\n/**\n * apiRefresh\n *\n * @param {Obj} payload\n * @returns {Promise}\n */\nexport function apiRefresh(payload) {\n\treturn new Promise((resolve, reject) => {\n\t\taxios({\n\t\t\tmethod: 'post',\n\t\t\turl: process.env.API_BASE_URL + 'oauth/token',\n\t\t\tdata: {\n\t\t\t\tgrant_type: process.env.OAUTH_REFRESH_GRANT_TYPE,\n\t\t\t\tclient_id: process.env.OAUTH_CLIENT_ID,\n\t\t\t\tclient_secret: process.env.OAUTH_CLIENT_SECRET,\n\t\t\t\trefresh_token: payload.refreshToken\n\t\t\t}\n\t\t})\n\t\t.then(function (response) {\n\t\t\tresolve(response);\n\t\t})\n\t\t.catch(function (error) {\n\t\t\treject(error.response);\n\t\t});\n\t});\n}\n\n\nfunction replaceApiVariables(str: any, userTokens: any) {\n\tlet newString = str;\n\tlet re = /{([^}]+)}/g, variable;\n\t// while (variable = re.exec(str)) {\n\t// \tif (variable[1].toLowerCase() == 'userid') {\n\n\t// \t\tnewString = newString.replace('{' + variable[1] + '}', userTokens.token_id);\n\t// \t}\n\t// \tif (variable[1].toLowerCase() == 'teamid') {\n\t// \t\tnewString = newString.replace('{' + variable[1] + '}', apiConstants.userId);\n\t// \t}\n\t// \tif (variable[1].toLowerCase() == 'languageId') {\n\t// \t\tnewString = newString.replace('{' + variable[1] + '}', apiConstants.language_id);\n\t// \t}\n\t// }\n\treturn newString;\n}\n\n/**\n * If the URL isn't already full base, prepend API_BASE_URL and return a full base URL\n *\n * @author Arithran Thurairetnam <arithran.thurairetnam@adp.com>\n * @param {string} url URL\n * @returns {string}\n */\nfunction getFullBaseUrl(url: any) {\n\tconst regExp = /^https?:\\/\\//i;\n\n\treturn regExp.test(url) ? url : apiDomain + url;\n}\n\nexport default {\n\tapiPut,\n\tapiPost,\n\tapiDelete,\n\tapiGet,\n\tapiLogin,\n\tapiRefresh,\n\treplaceApiVariables\n}\n"],"sourceRoot":""}]}